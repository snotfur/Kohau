-- Scripts:

local function PDAL_fake_script() -- Bill.Script 
	local script = Instance.new('Script', Bill)

	function lclick()
		
		game.Workspace.Boards:Destroy()
		
	end
	
	script.Parent.MouseButton1Click:Connect(lclick)
end
coroutine.wrap(PDAL_fake_script)()
local function HUNC_fake_script() -- Clear.Script 
	local script = Instance.new('Script', Clear)

	function lclick()
	
		local args = {
			[1] = 1,
			[2] = workspace.Assets.Cooking.Trashcans.TrashCan.Interact,
			[3] = "Trash Food"
		}
		
		game:GetService("ReplicatedStorage").Packages.Knit.Services.InteractionService.RF.Clicked:InvokeServer(unpack(args))
		
	end
	
	script.Parent.MouseButton1Click:Connect(lclick)
end
coroutine.wrap(HUNC_fake_script)()
