local function CSVLE_fake_script() -- Appetizers.Script 
	local script = Instance.new('Script', Appetizers)

	function lclick()
		local args = {
			[1] = "",
			[2] = "All"
		}
	
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
	end
	
	script.Parent.MouseButton1Click:Connect(lclick)
end
coroutine.wrap(CSVLE_fake_script)()
local function UQPVH_fake_script() -- Left.Script 
	local script = Instance.new('Script', Left)

	Left = script.Parent
	
	MS = game.CoreGui.Main.Backing.Pages.MS
	Settings = game.CoreGui.Main.Backing.Pages.Settings
	Speech = game.CoreGui.Main.Backing.Pages.Speech
	SecondSpeech = game.CoreGui.Main.Backing.Pages.SecondSpeech
	
	function lclick()
		if MS.Visible == true then
			Settings.Visible = true
			MS.Visible = false
		elseif Speech.Visible  == true then
			Speech.Visible = false
			MS.Visible = true
		elseif SecondSpeech.Visible == true then
			SecondSpeech.Visible = false
			Speech.Visible = true
		end
	end
	
	Left.MouseButton1Click:Connect(lclick)
end
coroutine.wrap(UQPVH_fake_script)()
local function VXTDPWN_fake_script() -- Right.Script 
	local script = Instance.new('Script', Right)

	Right = script.Parent
	
	MS = game.CoreGui.Main.Backing.Pages.MS
	Settings = game.CoreGui.Main.Backing.Pages.Settings
	Speech = game.CoreGui.Main.Backing.Pages.Speech
	SecondSpeech = game.CoreGui.Main.Backing.Pages.SecondSpeech
	
	function lclick()
		if Settings.Visible == true then
			MS.Visible = true
			Settings.Visible = false
		elseif MS.Visible == true then 
			MS.Visible = false
			Speech.Visible = true
		elseif Speech.Visible == true then
			Speech.Visible = false
			SecondSpeech.Visible = true
		end
	end
	
	Right.MouseButton1Click:Connect(lclick)
end
coroutine.wrap(VXTDPWN_fake_script)()
