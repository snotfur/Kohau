local function EUTLOHP_fake_script() -- Left.LocalScript 
	local script = Instance.new('LocalScript', Left)

	local Left = script.Parent
	
	local MS = game.CoreGui.Main.Backing.Pages.MS
	local Settings = game.CoreGui.Main.Backing.Pages.Settings
	
	function lclick()
		if Settings.Visible == false then
			Settings.Visible = true
			MS.Visible = false
		end
	end
	
	Left.MouseButton1Click:Connect(lclick)
end
coroutine.wrap(EUTLOHP_fake_script)()
local function JJCAE_fake_script() -- Right.LocalScript 
	local script = Instance.new('LocalScript', Right)

	local Right = script.Parent
	
	local MS = game.CoreGui.Main.Backing.Pages.MS
	local Settings = game.CoreGui.Main.Backing.Pages.Settings
	
	function lclick()
		if Settings.Visible == true then
			Settings.Visible = false
			MS.Visible = true
		end
	end
	
	Right.MouseButton1Click:Connect(lclick)
end
coroutine.wrap(JJCAE_fake_script)()
