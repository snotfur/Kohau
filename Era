-- Scripts:

local function HDGGKF_fake_script() -- Bill.Script 
	local script = Instance.new('Script', Bill)

	function lclick()
		game.Workspace.Billboards:Destroy()
	end
	
	script.Parent.MouseButton1Click:Connect(lclick)
	
end
coroutine.wrap(HDGGKF_fake_script)()
local function YYBHPCF_fake_script() -- Night.Script 
	local script = Instance.new('Script', Night)

	function lclick()
		sethiddenproperty(game.Lighting, "Technology", "Future")
	
		workspace.DescendantAdded:Connect(function(v)
			if v:IsA("SpotLight") or v:IsA("SurfaceLight") or v:IsA("PointLight") then
				v.Shadows = true
			end
		end)
	
		game.Lighting.ClockTime = 0
		game.Lighting.Ambient = Color3.new(0.301961, 0.301961, 0.301961)
		game.Lighting.Brightness = 1
		game.Lighting.OutdoorAmbient = Color3.new(0,0,0)
		game.Lighting.FogColor = Color3.new(0,0,0)
	end
	
	script.Parent.MouseButton1Click:Connect(lclick)
	
	
end
coroutine.wrap(YYBHPCF_fake_script)()
local function VCDD_fake_script() -- Intro.Script 
	local script = Instance.new('Script', Intro)

	function lclick()
		local args = {
			[1] = "Hello, and welcome to Kohaú. My name is Kenny, and I will be serving you today. How many people are in your party?",
			[2] = "All"
		}
	
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
	end
	
	script.Parent.MouseButton1Click:Connect(lclick)
end
coroutine.wrap(VCDD_fake_script)()
